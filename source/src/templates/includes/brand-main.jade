div.modal.fade(id="set_economical", tabindex="-1", role="dialog" aria-labelledby="economicalLabel", aria-hidden="true")
    div.modal-dialog(role="document")
        div.modal-content
            div.modal-header.py-2
                h5.modal-title(id="economicalLabel") В состав комплекта входят:
                button.close(type="button", data-dismiss="modal", aria-label="Close")
                span(aria-hidden="true") &times;
            div.modal-body
                ol
                    li
                        span Комплект крепежа
                    li
                        span Пневморессоры производства Fabio или Connect или любой другой производитель
                    li
                        span Пластиковая трубка высокого давления производство Италия
                    li
                        span Штуцера Италия
                    li
                        span Нипель на подкачку

