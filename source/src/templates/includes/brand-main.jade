section.brand.nn-container.mb-4.mb-lg-5.px-3.px-lg-0
    h2.brand__title.my-5.text-center
    div.row.justify-content-around
        div.col-12.col-lg-5.text-center.slide-fwd-center.mb-5.mb-lg-0
            img(src="/dist/img/cars/ford-transit.jpg").brand__img.img-fluid.w-100
        div.col-12.col-lg-6.align-self-center
            div.set
            table.table.table-hover
                tbody
                    tr
                        td.set__name Эконом
                            span.icon_info(data-toggle="modal", data-target="#set_economical")
                            //span.icon_info(data-toggle="tooltip", data-placement="right",title="состав комплекта")
                        td.text-right 5500грн
                    tr
                        td.set__name Бюджет
                            span.icon_info(data-toggle="modal", data-target="#set_budget")
                        td.text-right 8000грн
                    tr
                        td.set__name Стандарт
                            span.icon_info(data-toggle="modal", data-target="#set_standard")
                            span.brand__hit.d-inline-block.swing-in-left-fwd.ml-5
                                img(src="/dist/img/icons/hit.png").brand__hit.img-fluid
                        td.text-right 12800грн
                    tr.border-bottom
                        td.set__name Премиум
                            span.icon_info(data-toggle="modal", data-target="#set_premium")
                        td.text-right от 18000грн